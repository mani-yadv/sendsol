# SendSol Environment Variables Example
# Copy this to .env and fill in your values

# Solana Publishing (Production)
SOLANA_MAINNET_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_DEVNET_RPC_URL=https://api.devnet.solana.com
ANDROID_SDK_BUILD_TOOLS=/usr/lib/android-sdk/build-tools/debian

# Solana Keypair Paths (DO NOT COMMIT ACTUAL KEYPAIRS)
PUBLISHER_KEYPAIR_MAINNET=publishing/sendsol-publisher-mainnet.json
PUBLISHER_KEYPAIR_DEVNET=publishing/sendsol-publisher.json

# Java Environment
JAVA_HOME=/usr/lib/jvm/java-17-openjdk-amd64

# App Configuration
APP_VERSION=0.0.1
APP_PACKAGE_NAME=app.sendsol.www.twa
APP_DOMAIN=sendsol.app

# Store Publishing
STORE_NETWORK=mainnet
PRIORITY_FEE_LAMPORTS=500000