# Solana dApp Store Publishing Environment
# Copy this to .env for dapp-store CLI configuration

# Android SDK Build Tools Path
ANDROID_SDK_BUILD_TOOLS=/usr/lib/android-sdk/build-tools/debian

# Java Environment  
JAVA_HOME=/usr/lib/jvm/java-17-openjdk-amd64

# RPC Configuration
SOLANA_MAINNET_RPC=https://api.mainnet-beta.solana.com
SOLANA_DEVNET_RPC=https://api.devnet.solana.com

# Priority Fee (lamports)
PRIORITY_FEE=500000