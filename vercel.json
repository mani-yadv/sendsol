{
  "routes": [
    {
      "src": "/app-manifest\\.json",
      "dest": "/app-manifest.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/(logo\\.png|icon-192x192\\.png|icon-512x512\\.png|icon\\.svg)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "image/png"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}