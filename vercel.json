{
  "functions": {
    "server/api/transactions/get.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/manifest\\.json",
      "dest": "/manifest.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/(logo\\.png|icon-192x192\\.png|icon-512x512\\.png|icon\\.svg)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "image/png"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}